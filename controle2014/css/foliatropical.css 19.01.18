@charset "utf-8";
/* CSS Document */
html, body, div, span, applet, object, iframe, table, caption,
tbody, tfoot, thead, tr, th, td, del, dfn, em, font, img, ins,
kbd, q, s, samp, small, strike, strong, sub, sup, tt, var,
h1, h2, h3, h4, h5, h6, p, blockquote, pre, a, abbr,
acronym, address, big, cite, code, dl, dt, dd, ol, ul, li,
fieldset, form, label, legend {
    vertical-align: baseline;
    font-family: inherit;
    font-weight: inherit;
    font-style: inherit;
    font-size: 100%;
    outline: 0;
    padding: 0;
    margin: 0;
    border: 0;
}
:focus { outline: 0; }
body {
    background: white;
    line-height: 1;
    color: black;
}
ol, ul { list-style: none; }
table {
    border-collapse: separate;
    border-spacing: 0;
}
table td { /*vertical-align: middle;*/}
caption, th, td {
    font-weight: normal;
    text-align: left;
}
blockquote:before, blockquote:after, q:before, q:after { content: ""; }
blockquote, q { quotes: "" ""; }
strong { font-weight: bold; }
em { font-style: italic; }
header, section, footer, article, nav, aside { display: block; }
h1, h2, h3, h4, h5, h6 { font-weight: 400; }

/*------------------------------ Global --------------------------------*/

body {      
    font: normal 12px/1em 'Open Sans', sans-serif;
    position: relative;
    text-align: center;
    margin: 0;
    padding: 0;
    background: #f4f7f9;
}

/*----------------------------------------------------------------*/

h1 {
    font-size: 30px;
    line-height: 1em;
    color: #757575;
}
h3 {
    font-size: 14px;
    line-height: 1em;
    color: #757575;
}
h1 span { color: #16b2ff; }
h3 span { color: #7f9fca; }

/*------------------------------Header Topo----------------------------------*/

header#topo {
    position: relative;
    width: 100%;
    display: block;
    background: #fff;
    border-bottom: 1px #dce0e3 solid;
    z-index: 2;
}

    header#topo section#logado {
        position: relative;
        height: 30px;
        background: #16b2ff;
        z-index: 3;
    }
        header#topo section#logado .wrapper {
            position: relative;
            padding-right: 180px;
            width: 780px;
            height: 30px;
            margin: 0 auto;
            color: #fff;

            font-size: 10px;
            font-weight: 600;
            line-height: 30px;
            text-transform: uppercase;
            text-align: right;
        }
        header#topo section#logado .wrapper a.logout {
            display: block;
            position: absolute;
            right: 150px;
            top: 50%;
            margin-top: -15px;
            width: 30px;
            height: 30px;
            background: url(../img/bg-logout.png) no-repeat center center;
            text-decoration: none;
        }

        /*------------------------------Busca voucher----------------------------------*/

        header#topo section#logado .wrapper section#busca-voucher {
            position: absolute;
            left: 0;
            top: 0;
            padding: 6px 0;
        }
            header#topo section#logado .wrapper section#busca-voucher a.show {
                position: relative;
                display: block;
                width: 16px;
                height: 16px;
                background: url(../img/bg-busca-voucher.png) no-repeat 0 0;
                text-decoration: none;
                z-index: 2;
            }
            header#topo section#logado .wrapper section#busca-voucher form {
                display: none;
                position: absolute;
                left: 0;
                top: 0;
                z-index: 1;
            }
            header#topo section#logado .wrapper section#busca-voucher form label {
                position: absolute;
                left: 15px;
                top: 50%;
                margin-top: -0.5em;
                font-size: 10px;
                font-weight: 600;
                line-height: 1em;
                color: #16b2ff;
            }
            header#topo section#logado .wrapper section#busca-voucher form input.input {
                width: 150px;
                padding: 0 15px;
                border: none;
                background: #fff;
                color: #16b2ff;
                text-decoration: none;

                font: 600 10px/20px 'Open Sans', sans-serif;
                height: 20px;
                
                border-radius: 10px;
                -moz-border-radius: 10px;
                -webkit-border-radius: 10px;

                -webkit-transition: background-color .3s;
                -moz-transition: background-color .3s;
                -o-transition: background-color .3s;
                transition: background-color .3s;
            }
            header#topo section#logado .wrapper section#busca-voucher form input.submit {
                position: absolute;
                right: 5px;
                top: 50%;
                border: 0;
                margin: -8px 0 0 0;
                padding: 0;
                width: 16px;
                height: 16px;
                text-decoration: none;
                background: url(../img/bg-busca-voucher.png) no-repeat 0 -16px;
                cursor: pointer;
            }



        /*------------------------------Selecionar o evento----------------------------------*/

        header#topo section#logado .wrapper section#header-muda-carnaval {
            position: absolute;
            right: 10px;
            top: 50%;
            margin-top: -10px;
            width: 130px;
            height: 20px;
            text-transform: uppercase;
            text-align: left;
        }
            header#topo section#logado .wrapper section#header-muda-carnaval a.arrow {
                display: block;
                padding: 0 20px 0 10px;
                background: #fff url(../img/bg-muda-evento.png) no-repeat 115px center;
                color: #16b2ff;
                text-decoration: none;

                font-size: 10px;
                font-weight: 600;
                line-height: 20px;
                height: 20px;
                overflow: hidden;

                border-radius: 10px;
                -moz-border-radius: 10px;
                -webkit-border-radius: 10px;

                -webkit-transition: background-color .3s;
                -moz-transition: background-color .3s;
                -o-transition: background-color .3s;
                transition: background-color .3s;
            }
            header#topo section#logado .wrapper section#header-muda-carnaval a.arrow:hover { background-color: #e4f5ff; }
                header#topo section#logado .wrapper section#header-muda-carnaval.open a.arrow {
                    background-color: #fff;

                    border-radius: 10px 10px 0 0;
                    -moz-border-radius: 10px 10px 0 0;
                    -webkit-border-radius: 10px 10px 0 0;
                }
            header#topo section#logado .wrapper section#header-muda-carnaval ul.drop {
                display: none;
                position: absolute;
                left: -1px;
                top: 20px;
                width: 130px;
                padding: 5px 0 10px 0;
                background: #fff;
                border: 1px #16b2ff solid;
                border-top-color: #f0f0f0;

                border-radius: 0 0 10px 10px;
                -moz-border-radius: 0 0 10px 10px;
                -webkit-border-radius: 0 0 10px 10px;
            }
                header#topo section#logado .wrapper section#header-muda-carnaval ul.drop li a {
                    display: block;
                    padding: 0 10px;

                    font-size: 10px;
                    font-weight: 600;
                    line-height: 20px;

                    color: #16b2ff;
                    text-decoration: none;

                    -webkit-transition: .3s;
                    -moz-transition: .3s;
                    -o-transition: .3s;
                    transition: .3s;
                }
                header#topo section#logado .wrapper section#header-muda-carnaval ul.drop li a:hover {
                    color: #fff;
                    background-color: #16b2ff;
                }

    /*------------------------------Header Topo----------------------------------*/

    header#topo nav {
        display: block;
        position: relative;
        width: 790px;
        height: 100px;
        padding-left: 190px;
        margin: 0 auto;
        text-align: left;
    }
        header#topo nav a#logo {
            position: absolute;
            top: 0;
            left: 0;
            width: 105px;
            height: 100px;
            background: url(../img/logo.jpg) no-repeat center center;
        }
        header#topo nav ul {
            height: 50px;
            padding: 25px 0;
            position: relative;
        }
            header#topo nav ul li {
                display: block;
                float: left;
            }
                header#topo nav ul li a {                   
                    display: block;
                    text-decoration: none;
                    font-size: 12px;
                    font-weight: 600;
                    color: #6f8797;
                    line-height: 50px;
                    padding: 0 8px;
                    background: url(../img/bg-nav-selected.png) no-repeat center 60px;

                    -webkit-transition: color .3s;
                    -moz-transition: color .3s;
                    -o-transition: color .3s;
                    transition: color .3s;
                }
                header#topo nav ul li a:hover {
                    color: #16b2ff;
                }
                header#topo nav ul li.drop:hover > a,
                header#topo nav ul li.drop.over > a { background-position: center bottom; }
                header#topo nav ul li.drop:hover > ul,
                header#topo nav ul li.drop:hover > ul.hidden,
                header#topo nav ul li.drop.over > ul,
                header#topo nav ul li.drop.over > ul.hidden {
                    display: block;
                }
                    header#topo nav ul li.drop ul {
                        /*display: none;*/
                        position: absolute;
                        /*width: 100%;*/
                        top: 75px;
                        left: auto;
                        padding: 0;
                    }
                    header#topo nav ul li.drop ul.hidden { display: none; }
                    header#topo nav ul li.drop ul li {
                        background: #16b2ff;
                        box-shadow: 3px 3px 0 rgba(0,0,0,.05);
                        -moz-box-shadow: 3px 3px 0 rgba(0,0,0,.05);
                        -webkit-box-shadow: 3px 3px 0 rgba(0,0,0,.05);
                    }
                    header#topo nav ul li.drop ul li a {
                        color: #fff;
                        padding: 0 15px 0 0;
                    }
                    header#topo nav ul li.drop ul li a:hover { color: #98d5ff; }
                    header#topo nav ul li.drop ul li a.first-child { padding-left: 15px; }
                    
                    header#topo nav ul li.drop.right ul li {
                        float: right;
                        box-shadow: -3px 3px 0 rgba(0,0,0,.05);
                        -moz-box-shadow: -3px 3px 0 rgba(0,0,0,.05);
                        -webkit-box-shadow: -3px 3px 0 rgba(0,0,0,.05);
                    }

/*----------------------------------------------------------------*/

header#topo.index {
    background: #fff url(../img/logo.jpg) no-repeat center center;
    height: 100px;
}

/*----------------------------------------------------------------*/

section#page-login {
    position: fixed;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
    background: url(../img/bg-overlay.png);
    z-index: 3;
}
    section#page-login form#form-login {
        position: absolute;
        left: 50%;
        top: 50%;
        margin-left: -180px;
        margin-top: -180px;
        width: 280px;
        height: 200px;
        padding: 30px 40px;
        background: #f1f1f1;
        text-align: left;

        border-radius: 5px;
        -moz-border-radius: 5px;
        -webkit-border-radius: 5px;

        box-shadow: 0 0 15px rgba(0,0,0,.05);
        -moz-box-shadow: 0 0 15px rgba(0,0,0,.05);
        -webkit-box-shadow: 0 0 15px rgba(0,0,0,.05);
    }
        section#page-login form#form-login h1 {
            font-size: 30px;
            line-height: 1em;
            color: #757575;
            margin-bottom: 30px;
        }
        section#page-login form#form-login p {
            position: relative;
            margin-bottom: 10px;
        }
            section#page-login form#form-login p label.infield {
                position: absolute;
                bottom: 0;
                left: 40px;
                font-size: 14px;
                font-weight: 600;
                height: 45px;
                line-height: 45px;
                color: #c0c4ca;
                margin: 0;
                cursor: text;
            }
            section#page-login form#form-login p input.input {
                margin: 0;
                border: 1px #bcd0ec solid;
                padding: 4px 19px 4px 39px;
                width: 220px;
                height: 35px;
                text-decoration: none;
                font: 600 14px/35px 'Open Sans', sans-serif;
                color: #16b2ff;
                background: #fff url(../img/bg-input-login.png) no-repeat 0 0;

                border-radius: 6px;
                -moz-border-radius: 6px;
                -webkit-border-radius: 6px;

                -webkit-transition: color, border-color, background-color .3s;
                -moz-transition: color, border-color, background-color .3s;
                -o-transition: color, border-color, background-color .3s;
                transition: color, border-color, background-color .3s;
            }
            section#page-login form#form-login p input.input#login { background-position: 0 0; }
            section#page-login form#form-login p input.input#senha { background-position: 0 -45px; }

            /*----------------------------------------------------------------*/

            section#page-login form#form-login p input.submit {
                position: absolute;
                right: 0;
                top: 0;
                margin: 0;
                padding: 0;
                border: 2px #72a14c solid;
                width: 55px;
                height: 45px;
                font: 700 14px/1em 'Open Sans', sans-serif;
                text-shadow: 1px 1px 0 rgba(0,0,0,.3);
                text-align: center;
                text-transform: uppercase;
                color: #fff;
                background: #96c86e url(../img/bg-botao-brilho.png) no-repeat center center;
                cursor: pointer;

                border-radius: 0 6px 6px 0;
               -moz-border-radius: 0 6px 6px 0;
               -webkit-border-radius: 0 6px 6px 0;

                -webkit-transition: .3s;
                -moz-transition: .3s;
                -o-transition: .3s;
                transition: .3s;
            }
            section#page-login form#form-login p input.submit:hover { background-color: #a3d57b; } 

        /*----------------------------------------------------------------*/

        section#page-login form#form-login span.erro {
            display: block;
            padding-top: 10px;
            height: 20px;
            font-size:15px;
            line-height: 20px;
            color: #d5005e;
            text-align: right;
        }

/*----------------------------------------------------------------*/

section#conteudo {
    position: relative;
    width: 1060px;
    /*width: 980px;*/
    margin: 0 auto;
    text-align: left;
    z-index: 1;
}
section#conteudo.larger {
    width: 80%;
}
section#conteudo.relatorio-lista-voucher { width: 800px; }
section#conteudo.relatorio-lista-voucher.wide { width: 1420px; }

    /*----------------------------------------------------------------*/

    section#conteudo .coluna {
        float: left;
        margin-right: 10px !important;
    }

    /*---------------------- Padr√£o de estilos -----------------------*/

    section#conteudo header.titulo {
        padding: 30px 10px;
        border-bottom: 1px #c9c9c9 dashed;
        position: relative;
    }
        section#conteudo header.titulo a.exportar-relatorio {
            position: absolute;
            right: 10px;
            bottom: 15px;
            text-decoration: none;
            font-size: 14px;
            font-weight: 600;
            line-height: 45px;
            height: 45px;
            color: #16b2ff;
            cursor: pointer;
            overflow: hidden;
        }
        section#conteudo header.titulo .valor-total {
            position: absolute;
            right: 10px;
            top: 30px;
            font-size: 30px;
            line-height: 1em;
            color: #757575;
        }
        section#conteudo header.titulo form.busca-lista {
            position: absolute;
            right: 0;
            bottom: 15px;
        }
            section#conteudo header.titulo form.busca-lista a.adicionar {
                display: block;
                float: left;
                text-decoration: none;
                margin: 0 10px 0 0;
                padding: 0;
                border: 2px #72A14C solid;
                width: 40px;
                height: 40px;
                font: 700 27px/40px 'Open Sans', sans-serif;
                text-shadow: 1px 1px 0 rgba(0, 0, 0, 0.3);
                text-align: center;
                color: #FFF;
                background: #96C86E url(../img/bg-seleciona-alocacao-brilho.png) no-repeat right bottom;

                border-radius: 6px;
                -moz-border-radius: 6px;
                -webkit-border-radius: 6px;

                -webkit-transition: .3s;
                -moz-transition: .3s;
                -o-transition: .3s;
                transition: .3s;
            }
            section#conteudo header.titulo form.busca-lista a.adicionar:hover { background-color: #a3d57b;  }
        section#conteudo header.titulo form.busca-lista p label { color: #7f9fca; }
        section#conteudo header.titulo form.busca-lista p input.input {
            width: 120px;
            color: #7f9fca;
            padding-right: 40px;
        }
        section#conteudo header.titulo form.busca-lista input.submit {
            position: absolute;
            right: 20px;
            top: 50%;
            border: 0;
            margin: -12px 0 0 0;
            padding: 0;
            width: 25px;
            height: 25px;
            text-decoration: none;
            background: url(../img/sprite-alocacao.png) no-repeat left top;
            cursor: pointer;
        }
        section#conteudo header.titulo form.busca-lista p a.limpar-busca {
            display: block;
            position: absolute;
            top: 50%;
            right: 38px;
            margin-top: -0.5em;
            color: #7f9fca;
            font-size: 20px;
            font-weight: 800;
            line-height: 1em;
            text-decoration: none;

            -webkit-transition: .3s;
            -moz-transition: .3s;
            -o-transition: .3s;
            transition: .3s;

        }
        section#conteudo header.titulo form.busca-lista p a.limpar-busca:hover { color: #16b2ff }
        

    /*----------------------------------------------------------------*/

    section#conteudo section.secao {
        padding: 30px 10px;
        border-bottom: 1px #c9c9c9 dashed;
    }
    section#conteudo section.secao.label-top { padding-top: 10px; }
    section#conteudo section.secao.bottom { border-bottom-style: solid; }

    /*----------------------------------------------------------------*/

    section#conteudo footer.controle {
        text-align: right;
        padding: 30px 10px;
        border-bottom: 1px #c9c9c9 solid;
    }
        section#conteudo footer.controle .coluna {
            float: right;
            margin-right: 0 !important;
            margin-left: 10px !important;
        }

    /*----------------------------------------------------------------*/

    section#conteudo a.button,
    section#conteudo a.cancel {
        display: block;
        border: 2px #9ba5ad solid;
        width: 111px;
        height: 41px;
        font-size: 16px;
        font-weight: 700;
        line-height: 41px;
        text-decoration: none;
        text-shadow: 1px 1px 0 rgba(0,0,0,.3);
        text-align: center;
        color: #fff;
        background: #b7c2ca url(../img/bg-botao-brilho.png) no-repeat center center;

        border-radius: 6px;
       -moz-border-radius: 6px;
       -webkit-border-radius: 6px;

        -webkit-transition: .3s;
        -moz-transition: .3s;
        -o-transition: .3s;
        transition: .3s;
    }
    section#conteudo a.cancel:hover { background-color: #c5cfd6; } 
    section#conteudo a.button.big { width: 130px; }

    section#conteudo a.button {
        background-color: #96c86e;
        border-color: #72a14c;
    }
    section#conteudo a.button:hover { background-color: #a3d57b; } 
    
    section#conteudo a.button.modificar {
        width: 170px;
        background-position: right center;
    }
    section#conteudo a.button.cancelar-compra {
        width: 170px;
        background-color: #b83a3a;
        border-color: #861a1a;
        background-position: right center;
    }
    section#conteudo a.button.cancelar-compra.desfazer { width: 190px; }
    section#conteudo a.button.cancelar-compra:hover { background-color: #d33333; } 


    section#conteudo input.submit {
        margin: 0;
        padding: 0;
        border: 2px #72a14c solid;
        width: 115px;
        height: 45px;
        font: 700 16px/1em 'Open Sans', sans-serif;
        text-shadow: 1px 1px 0 rgba(0,0,0,.3);
        text-align: center;
        color: #fff;
        background: #96c86e url(../img/bg-botao-brilho.png) no-repeat center center;
        cursor: pointer;

        border-radius: 6px;
       -moz-border-radius: 6px;
       -webkit-border-radius: 6px;

        -webkit-transition: .3s;
        -moz-transition: .3s;
        -o-transition: .3s;
        transition: .3s;
    }
    a.submit { 
        margin: 0;
        padding: 0;
        border: 2px #72a14c solid;
        width: 115px;
        height: 41px;
        font: 700 16px/1em 'Open Sans', sans-serif;
        text-shadow: 1px 1px 0 rgba(0,0,0,.3);
        text-align: center;
        color: #fff;
        background: #96c86e url(../img/bg-botao-brilho.png) no-repeat center center;
        cursor: pointer;

        border-radius: 6px;
       -moz-border-radius: 6px;
       -webkit-border-radius: 6px;

        -webkit-transition: .3s;
        -moz-transition: .3s;
        -o-transition: .3s;
        transition: .3s;
        text-decoration: none;
        width: 210px;
        background-position: right center;
        line-height: 41px;
        display: block;
    }
    section#conteudo input.submit.adicionar {
        width: 45px;
        font-size: 24px;
        background-image: url(../img/bg-botao-quadrado-brilho.png);
    }
    section#conteudo input.submit:hover { background-color: #a3d57b; } 
    section#conteudo input.submit.cancelar-expedicao {
        background-color: #b83a3a;
        border-color: #861a1a;
    }
    section#conteudo input.submit.cancelar-expedicao:hover { background-color: #d33333; } 

    /*----------------------------------------------------------------*/

    section#conteudo .checkbox.infield,
    section#conteudo .radio.infield { position: relative; }

        section#conteudo .checkbox.infield h3,
        section#conteudo .radio.infield h3 {
            height: 30px;
            line-height: 30px;
            margin-bottom: 5px;
        }

        section#conteudo .checkbox.infield ul li,
        section#conteudo .radio.infield ul li {
            float: left;
            margin-right: 4px;
        }
            section#conteudo .checkbox.infield ul li label.item,
            section#conteudo .radio.infield ul li label.item {
                position: relative;
                display: block;
                width: 55px;
                height: 55px;
                font-size: 18px;
                font-weight: 400;
                line-height: 55px;
                text-align: center;
                background: #b7c2ca url(../img/bg-combo-infield.png) no-repeat right bottom;
                color: #fff;
                cursor: pointer;

                border-radius: 2px;
                -moz-border-radius: 2px;
                -webkit-border-radius: 2px;

                -webkit-transition: color, background-color .3s;
                -moz-transition: color, background-color .3s;
                -o-transition: color, background-color .3s;
                transition: color, background-color .3s;

                overflow: hidden;
            }
            section#conteudo .checkbox.infield.big ul li label.item,
            section#conteudo .radio.infield.big ul li label.item {
                width: 90px;
                font-size: 14px;
                font-weight: 600;
                background-image: url(../img/bg-tipo-ingresso.png);
                background-position: center center;

                border-radius: 4px;
                -moz-border-radius: 4px;
                -webkit-border-radius: 4px;
            }
            section#conteudo .radio.infield.big.modificar ul li label.item { width: 58px; }

            section#conteudo .checkbox.infield ul li label.item.disabled,
            section#conteudo .radio.infield ul li label.item.disabled,
            section#conteudo .checkbox.infield ul li label.item.disabled:hover,
            section#conteudo .radio.infield ul li label.item.disabled:hover {
                color: #f0f0f0;
                background-color: #e1e6e7;
                cursor: default;
                text-shadow: -1px -1px 0 rgba(0,0,0,.1);
