    text-overflow: ellipsis;
}
section.enderecos ul.enderecos li a.editar {
    width: 25px;
    height: 26px;
    background: url(../img/carrinho-alterar-endereco.png) no-repeat left center;
    position: absolute;
    top: 10px;
    right: 10px;
}
section.enderecos ul.enderecos li .utilizar:hover {
    background: #16b2ff;
}
section.enderecos ul.enderecos li .utilizar {
    cursor: pointer;
    border: none;
    display: block;
    text-decoration: none;
    position: absolute;
    color: #fff;
    font-size: 12px;
    font-weight: 600;
    bottom: 20px;
    left: 20px;
    background: #16b2ff;
    padding: 15px 0;
    text-align: center;
    text-transform: uppercase;
    letter-spacing: 0.05em;
    width: 250px;
    -webkit-transition: .3s;
    -moz-transition: .3s;
    -o-transition: .3s;
    transition: .3s;
    border-radius: 5px;
    -moz-border-radius: 5px;
    -webkit-border-radius: 5px;
    box-shadow: 0 2px 0 rgba(0,0,0,0.2);
    -moz-box-shadow: 0 2px 0 rgba(0,0,0,0.2);
    -webkit-box-shadow: 0 2px 0 rgba(0,0,0,0.2);
}
section.enderecos ul.enderecos li input[type="radio"]{
