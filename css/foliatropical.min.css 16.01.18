@charset "utf-8";
/* CSS Document */
html, body, div, span, applet, object, iframe, table, caption,
tbody, tfoot, thead, tr, th, td, del, dfn, em, font, img, ins,
kbd, q, s, samp, small, strike, strong, sub, sup, tt, var,
h1, h2, h3, h4, h5, h6, p, blockquote, pre, a, abbr,
acronym, address, big, cite, code, dl, dt, dd, ol, ul, li,
fieldset, form, label, legend, figure {
    vertical-align: baseline;
    font-family: inherit;
    font-weight: inherit;
    font-style: inherit;
    font-size: 100%;
    outline: 0;
    padding: 0;
    margin: 0;
    border: 0;
}
:focus { outline: 0; }
body {
    background: white;
    line-height: 1;
    color: black;
}
ol, ul { list-style: none; }
table {
    border-collapse: separate;
    border-spacing: 0;
}
table td { /*vertical-align: middle;*/}
caption, th, td {
    font-weight: normal;
    text-align: left;
}
blockquote:before, blockquote:after, q:before, q:after { content: ""; }
blockquote, q { quotes: "" ""; }
strong { font-weight: bold; }
em { font-style: italic; }
header, section, footer, article, nav, aside, figure, main { display: block; }
h1, h2, h3, h4, h5, h6 { font-weight: 400; }

/*------------------------------ Global --------------------------------*/

body {      
    /*font: normal 12px/1em 'Open Sans', sans-serif;*/
    font: normal 12px/1em 'Open Sans Condensed', sans-serif;
    position: relative;
    text-align: center;
    margin: 0;
    padding: 150px 0 0 0;
    background: #fff url(../img/bg-body.png) repeat center top;
}

div#pop {
    position: fixed;
    z-index: 999;
    left: 50%;
    margin-left: -300px;
    top: 190px;
    display: block;
}
    div#pop .closepop {
        width: 55px;
        height: 55px;
        background: url(../img/closepop.png) no-repeat center center;
        position: absolute;
        top: -26px;
        right: -26px;
        z-index: 2;
    }

/*----------------------------------------------------------------*/

section#outter {
    position: relative;
    min-width: 980px;
    overflow: hidden;
}

/*----------------------------------------------------------------*/

header#topo {
    position: relative;
    height: 80px;
    z-index: 999;
}
    header#topo nav#principal {
        position: fixed;
        left: 0;
        top: 0;
        width: 100%;
        height: 80px;
        background: #f78711 url(../img/bg-header-nav.png) repeat center top;
        
        box-shadow: 0 4px 0 rgba(0,0,0,.1);
        -moz-box-shadow: 0 4px 0 rgba(0,0,0,.1);
        -webkit-box-shadow: 0 4px 0 rgba(0,0,0,.1);

        z-index: 999;
    }
        header#topo nav#principal ul {
            position: relative;
            width: 1050px;
            height: 80px;
            margin: 0 auto;
            text-align: left;
        }
            header#topo nav#principal ul li {
                float: left;
                padding: 25px 0;
                height: 30px;
            }
            header#topo nav#principal ul li a {
                display: block;
                padding: 0 8px;
                font-size: 15px;
                font-weight: 700;
                line-height: 30px;
                color: #ffe3c5;
                text-decoration: none;
                text-transform: uppercase;

                -webkit-transition: color .3s;
                -moz-transition: color .3s;
                -o-transition: color .3s;
                transition: color .3s;
            }
            header#topo nav#principal ul li a:hover { color: #fff; }        
            header#topo nav#principal ul li.login a { text-decoration: underline; }

            header#topo nav#principal ul li.logout a { text-decoration: underline; }

            /*----------------------------------------------------------------*/

            header#topo nav#principal ul li.drop {
                position: relative;
                padding: 20px 0;
                height: 40px;
            }
                header#topo nav#principal ul li.drop span.drop {
                    cursor: pointer;
                    display: block;
                    padding: 0 8px;

                    -webkit-transition: color .3s;
                    -moz-transition: color .3s;
                    -o-transition: color .3s;
                    transition: color .3s;
                }
                header#topo nav#principal ul li.drop a.drop,
                header#topo nav#principal ul li.drop span.drop {
                    height: 40px;
                    line-height: 40px;

                    border-radius: 3px 3px 0 0;
                    -moz-border-radius: 3px 3px 0 0;
                    -webkit-border-radius: 3px 3px 0 0;

                    -webkit-transition: color 0;
                    -moz-transition: color 0;
                    -o-transition: color 0;
                    transition: color 0;
                }
                header#topo nav#principal ul li.drop a.drop:hover,
                header#topo nav#principal ul li.drop:hover a.drop, 
                header#topo nav#principal ul li.drop span.drop:hover,
                header#topo nav#principal ul li.drop:hover span.drop {
                    background: #ffb700;
                    color: #9c5c2e;

                    box-shadow: 4px 4px 0 rgba(0,0,0,.1);
                    -moz-box-shadow: 4px 4px 0 rgba(0,0,0,.1);
                    -webkit-box-shadow: 4px 4px 0 rgba(0,0,0,.1);
                }
                header#topo nav#principal ul li.drop ul {
                    display: none;
                    position: absolute;
                    width: 200px;
                    height: auto;
                    padding: 10px 0;
                    top: 60px;
                    left: 0;
                    background: #ffb700;
                    z-index: 999;

                    border-radius: 0 3px 3px 3px;
                    -moz-border-radius: 0 3px 3px 3px;
                    -webkit-border-radius: 0 3px 3px 3px;

                    box-shadow: 4px 4px 0 rgba(0,0,0,.1);
                    -moz-box-shadow: 4px 4px 0 rgba(0,0,0,.1);
                    -webkit-box-shadow: 4px 4px 0 rgba(0,0,0,.1);
                }
                header#topo nav#principal ul li.drop:hover > ul { display: block; }
                header#topo nav#principal ul li.drop ul li {
                    display: block;
                    float: none;
                    height: 25px;
                    padding: 0;
                }
                header#topo nav#principal ul li.drop ul li a {
                    line-height: 25px;
                    height: 25px;
                    padding: 0 15px;
                    color: #9c5c2e;
                }
                header#topo nav#principal ul li.drop ul li a:hover { color: #cd8450; }

                header#topo.index nav#principal ul li.drop a.drop:hover,
                header#topo.index nav#principal ul li.drop:hover a.drop,
                header#topo.index nav#principal ul li.drop span.drop:hover,
                header#topo.index nav#principal ul li.drop:hover span.drop { background: #ffc816; }
                header#topo.index nav#principal ul li.drop ul { background: #ffc816; }

            /*----------------------------------------------------------------*/

            header#topo nav#principal ul li.logo {
                position: relative;
                padding: 0;
                width: 120px;
                height: 80px;
            }
                header#topo nav#principal ul li.logo a#logo {
                    display: block;
                    position: absolute;
                    left: -20px;
                    top: 5px;
                    width: 130px;
                    height: 70px;
                    background: url(../img/logomenu.png) no-repeat 0 0;
                }
                header#topo nav#principal ul li.logo a#logo span { display: none; }

            /*----------------------------------------------------------------*/
            
            header#topo nav#principal ul li.login {
                float: none;
                position: absolute;
                right: 150px;
                top: 0;
            }

            /*----------------------------------------------------------------*/
            
            header#topo nav#principal ul li.usuario {
                float: none;
                position: absolute;
                right: 195px;
                top: 0;

                font-size: 15px;
                font-weight: 700;
                line-height: 30px;
                color: #ffd1a0;
                text-transform: uppercase;
            }
            header#topo.index nav#principal ul li.usuario { color: #bb8245; }
            header#topo nav#principal ul li.usuario.us { right: 215px; }

            header#topo nav#principal ul li.logout {
                float: none;
                position: absolute;
                right: 150px;
                top: 0;
            }

            /*----------------------------------------------------------------*/

            header#topo nav#principal ul li.botao {
                float: none;
                position: absolute;
                right: 0;
                top: 0;
            }
            header#topo nav#principal ul li.botao.first { right: 90px; }
                header#topo nav#principal ul li.botao a {
                    color: #fff;
                    background-color: #c76700;
                    font: 700 10px/30px 'Open Sans', sans-serif;
                    text-align: center;

                    border-radius: 5px;
                    -moz-border-radius: 5px;
                    -webkit-border-radius: 5px;

                    box-shadow: 1px 1px 0 rgba(255,255,255,.2), inset 2px 3px 0 rgba(0,0,0,.1);
                    -moz-box-shadow: 1px 1px 0 rgba(255,255,255,.2), inset 2px 3px 0 rgba(0,0,0,.1);
                    -webkit-box-shadow: 1px 1px 0 rgba(255,255,255,.2), inset 2px 3px 0 rgba(0,0,0,.1);
                }
                header#topo nav#principal ul li.botao a:hover { background-color: #b35d01; }
                header#topo nav#principal ul li.botao a.carrinho {
                    padding: 0 5px 0 25px;
                    width: 20px;
                    background-image: url(../img/bg-header-carrinho.png);
                    background-repeat: no-repeat;
                    background-position: left center;
                }
                header#topo nav#principal ul li.botao a.lang {
                    position: relative;
                    padding: 0 20px 0 35px;
                    width: 25px;
                    background-image: url(../img/bg-arrow.png);
                    background-repeat: no-repeat;
                    background-position: 62px center;
                }
                    header#topo nav#principal ul li.botao a.lang span {
                        display: block;
                        position: absolute;
                        left: 8px;
                        top: 50%;
                        margin-top: -7px;
                        width: 21px;
                        height: 14px;
                        background: url(../img/bg-header-lang.png) no-repeat 0 0;
                    }
                    header#topo nav#principal ul li.botao a.lang.br span { background-position: 0 0; }
                    header#topo nav#principal ul li.botao a.lang.us span { background-position: 0 -14px; }

                /*----------------------------------------------------------------*/

                header#topo nav#principal ul li.botao .select { position: relative; }
                header#topo nav#principal ul li.botao .select.ativo a.lang.arrow {
                    border-radius: 5px 5px 0 0;
                    -moz-border-radius: 5px 5px 0 0;
                    -webkit-border-radius: 5px 5px 0 0;
                }
                    header#topo nav#principal ul li.botao .select .drop {
                        display: none;
                        position: absolute;
                        width: 80px;
                        top: 30px;
                        left: 0;
                    }
                    header#topo nav#principal ul li.botao .select .drop a.lang {
                        background-image: none;

                        border-radius: 0;
                        -moz-border-radius: 0;
                        -webkit-border-radius: 0;

                        box-shadow: 1px 1px 0 rgba(255,255,255,.2), inset 2px 1px 0 rgba(0,0,0,.1);
                        -moz-box-shadow: 1px 1px 0 rgba(255,255,255,.2), inset 2px 1px 0 rgba(0,0,0,.1);
                        -webkit-box-shadow: 1px 1px 0 rgba(255,255,255,.2), inset 2px 1px 0 rgba(0,0,0,.1);
                    }
                    header#topo nav#principal ul li.botao .select .drop a.lang.last {
                        border-radius: 0 0 5px 5px;
                        -moz-border-radius: 0 0 5px 5px;
                        -webkit-border-radius: 0 0 5px 5px;
                    }

                /*----------------------------------------------------------------*/

                header#topo nav#principal ul li a.disabled {
                    position: relative;
                    color: #ffb769;
                }
                header#topo.index nav#principal ul li a.disabled { color: #d48f45; }

                header#topo nav#principal ul li a.disabled span {
                    display: none;
                    position: absolute;
                    width: 100px;
                    height: auto;
                    padding: 10px 0;
                    top: 30px;
                    left: 50%;
                    margin-left: -50px;
                    background: #0090d2;
                    font-size: 14px;
                    line-height: 1em;
                    text-align: center;
                    color: #fff;
                    z-index: 999;

                    border-radius: 3px;
                    -moz-border-radius: 3px;
                    -webkit-border-radius: 3px;

                    box-shadow: 4px 4px 0 rgba(0,0,0,.1);
                    -moz-box-shadow: 4px 4px 0 rgba(0,0,0,.1);
                    -webkit-box-shadow: 4px 4px 0 rgba(0,0,0,.1);
                }
                header#topo nav#principal ul li a.disabled:hover {
                    background-image: url(../img/bg-menu-em-breve.png);
                    background-repeat: no-repeat;
                    background-position: center bottom;
                }
                header#topo nav#principal ul li a.disabled:hover span { display: block; }

                /*----------------------------------------------------------------*/

                header#topo nav#principal ul li.social {
                    position: absolute;
                    right: -95px;
                    top: 130px;
                    height: auto;
                    padding: 0;
                }
                    header#topo nav#principal ul li.social a,
                    header#topo nav#principal ul li.social a span {
                        display: block;
                        width: 35px;
                        height: 35px;
                        background-image: url(../img/bg-menu-social-sprite.png);
                        background-repeat: no-repeat;
                    }
                    header#topo nav#principal ul li.social a {
                        margin-bottom: 10px;
                        padding: 0;
                        background: url(../img/bg-menu-social.png) no-repeat right center;

                        border-radius: 5px;
                        -moz-border-radius: 5px;
                        -webkit-border-radius: 5px;

                        -webkit-transition: background-color .3s;
                        -moz-transition: background-color .3s;
                        -o-transition: background-color .3s;
                        transition: background-color .3s;
                    }
                    
                    header#topo nav#principal ul li.social a.whatsapp {
                        background-color: #65bc54;
                        
                        box-shadow: 0 3px 0 #257016;
                        -moz-box-shadow: 0 3px 0 #257016;
                        -webkit-box-shadow: 0 3px 0 #257016;
                    }
                    header#topo nav#principal ul li.social a.facebook {
                        background-color: #526ebb;
                        
                        box-shadow: 0 3px 0 #374f93;
                        -moz-box-shadow: 0 3px 0 #374f93;
                        -webkit-box-shadow: 0 3px 0 #374f93;
                    }
                    header#topo nav#principal ul li.social a.twitter {
                        background-color: #0c9aca;
                        
                        box-shadow: 0 3px 0 #067ba2;
                        -moz-box-shadow: 0 3px 0 #067ba2;
                        -webkit-box-shadow: 0 3px 0 #067ba2;
                    }
                    header#topo nav#principal ul li.social a.youtube {
                        background-color: #e9382c;
                        
                        box-shadow: 0 3px 0 #bb1a0f;
                        -moz-box-shadow: 0 3px 0 #bb1a0f;
                        -webkit-box-shadow: 0 3px 0 #bb1a0f;
                    }
                    header#topo nav#principal ul li.social a.instagram {
                        background-color: #7d665c;
                        
                        box-shadow: 0 3px 0 #5e483e;
                        -moz-box-shadow: 0 3px 0 #5e483e;
                        -webkit-box-shadow: 0 3px 0 #5e483e;
                    }
                    header#topo nav#principal ul li.social a.chat {
                        background-color: #89cd54;
                        
                        box-shadow: 0 3px 0 #69ab36;
                        -moz-box-shadow: 0 3px 0 #69ab36;
                        -webkit-box-shadow: 0 3px 0 #69ab36;
                    }

                    header#topo nav#principal ul li.social a.whatsapp span { background-position: -175px 0; }
                    header#topo nav#principal ul li.social a.facebook span { background-position: 0 0; }
                    header#topo nav#principal ul li.social a.twitter span { background-position: -35px 0; }
                    header#topo nav#principal ul li.social a.youtube span { background-position: -70px 0; }
                    header#topo nav#principal ul li.social a.instagram span { background-position: -105px 0; }
                    header#topo nav#principal ul li.social a.chat span { background-position: -140px 0; }

                    header#topo nav#principal ul li.social a.whatsapp:hover { background-color: #449234; }
                    header#topo nav#principal ul li.social a.facebook:hover { background-color: #5d7cd0; }
                    header#topo nav#principal ul li.social a.twitter:hover { background-color: #14a5d6; }
                    header#topo nav#principal ul li.social a.youtube:hover { background-color: #f54135; }
                    header#topo nav#principal ul li.social a.instagram:hover { background-color: #91786e; }
                    header#topo nav#principal ul li.social a.chat:hover { background-color: #98dd62; }

/*----------------------------------------------------------------*/

header#topo.index {
    /*height: 620px;*/
    height: 720px;
    background: #ff8400 url(../img/bg-header-index.png) repeat center top;
}
    header#topo.index nav#principal { background: #ffb700 url(../img/bg-header-nav-index.png) repeat center top; }
        header#topo.index nav#principal ul.menu {
            /*width: 850px;
            padding: 0 80px 0 50px;*/
            width: 880px;
            padding: 0 50px;
        }
            header#topo.index nav#principal ul li a { color: #9c5c2e; }
            header#topo.index nav#principal ul li a:hover { color: #cd8450; }

            /*header#topo.index nav#principal ul li a.ingress { color: #007CC9; }
            header#topo.index nav#principal ul li a.ingress:hover { color: #00A7E8; }*/

            nav#principal ul li a.ingress { color: #007CC9 !important;}
            nav#principal ul li a.ingress:hover { color: #00A7E8 !important; }

            header#topo.index nav#principal ul li.logo { display: none; }
            header#topo.index nav#principal ul li.login { right: 210px; }
            header#topo.index nav#principal ul li.usuario { right: 270px; }
            header#topo.index nav#principal ul li.usuario.us { right: 290px; }
            header#topo.index nav#principal ul li.logout { right: 225px; }
            header#topo.index nav#principal ul li.botao { right: 80px; }
            header#topo.index nav#principal ul li.botao.first { right: 170px; }
            header#topo.index nav#principal ul li.botao a {
                background-color: #bd7b14;
                color: #fff;
            }
            header#topo.index nav#principal ul li.botao a:hover { background-color: #a96d0f; }

        /*header#topo.index nav#principal ul,*/
        header#topo.index nav#principal ul li.logo,
        header#topo.index nav#principal ul li.botao,
        header#topo.index nav#principal ul li.login,
        header#topo.index nav#principal ul li.logout,
        header#topo.index nav#principal ul li.usuario,
        header#topo.index nav#principal ul li.botao.first {
            -webkit-transition: .3s;
            -moz-transition: .3s;
            -o-transition: .3s;
            transition: .3s;
        }

        header#topo.index.scroll nav#principal ul.menu {
            width: 980px;
            padding: 0;
        }
            header#topo.index.scroll nav#principal ul li.logo { display: block; }
            /*header#topo.index.scroll nav#principal ul li.login { right: 150px; }*/
            header#topo.index.scroll nav#principal ul li.login { right: 135px; }
            header#topo.index.scroll nav#principal ul li.usuario { right: 195px; }
            header#topo.index.scroll nav#principal ul li.usuario.us { right: 215px; }
            header#topo.index.scroll nav#principal ul li.logout { right: 150px; }
            header#topo.index.scroll nav#principal ul li.botao { right: 0; }
            header#topo.index.scroll nav#principal ul li.botao.first { right: 90px; }

    header#topo.index .outer {
        /*height: 540px;*/
        height: 640px;
        padding-top: 80px;
        background: url(../img/bg-header-brilho.jpg) no-repeat center 65px;
    }

    header#topo.index .wrapper {
        position: relative;
        width: 980px;
        /*height: 540px;*/
