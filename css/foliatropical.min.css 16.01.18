                height: 362px;
                z-index: 2;
            }
            header#topo.index .wrapper .parallax .splash-l {
                position: absolute;
                width: 390px;
                height: 595px;
                left: -110px;
                top: 20px;
                background: url(../img/bg-header-splash-l.png) no-repeat 0 0;
            }
            header#topo.index .wrapper .parallax .splash-r {
                position: absolute;
                width: 355px;
                height: 560px;
                right: -70px;
                top: 30px;
                background: url(../img/bg-header-splash-r.png) no-repeat 0 0;
            }

        /*----------------------------------------------------------------*/

        header#topo.index section#contador {
            position: absolute;
            bottom: 80px;
            left: 50%;
            margin-left: -200px;
            width: 400px;
            height: 80px;
            color: #fff;
        }
            header#topo.index section#contador ul li {
                float: left;
                width: 50px;
                margin: 0 15px;
                font-size: 14px;
                font-weight: 600;
                text-align: center;
                text-transform: uppercase;
            }
            header#topo.index section#contador ul li strong {
                display: block;
                font-size: 40px;
                font-weight: 600;
                line-height: 1em;
                margin-bottom: 10px;
            }

        /*----------------------------------------------------------------*/

        header#topo.index span.bottom-arrow {
            display: block;
            position: absolute;
            left: 50%;
            bottom: -60px;
            margin-left: -60px;
            width: 120px;
            height: 120px;
            text-decoration: none;
            background: url(../img/bg-header-down.png) no-repeat 0 0;
        }


/*----------------------------------------------------------------*/

section#comprar-tipos {
    position: relative;
    background: #ffc000 url(../img/bg-cubic-amarelo.png) repeat center top;
    padding-top: 40px;
    height: 560px;
    z-index: 1;
}
    section#comprar-tipos.slider a.ctrl { display: none; }

    section#comprar-tipos.slider .overflow,
    section#comprar-tipos.slider .overflow .list,
    section#comprar-tipos.slider .overflow .list .item,
    section#comprar-tipos.slider .overflow .list .item a.link,
    section#comprar-tipos.slider .overflow .list .item span.link {
        height: 500px;
    }

    section#comprar-tipos.slider .overflow {
        position: relative;
        width: 100%;
        overflow: hidden;
    }
        section#comprar-tipos.slider .overflow .list { width: 5000% }
        section#comprar-tipos.slider .overflow .list .item {
            float: left;
            width: 2%;
        }
            section#comprar-tipos.slider .overflow .list .item a.link,
            section#comprar-tipos.slider .overflow .list .item span.link {
                display: block;
                width: 1280px;
                position: relative;
                overflow: hidden;
                text-decoration: none;
                margin: 0 auto;
            }
                section#comprar-tipos.slider .overflow .list .item .link img {
                    display: block;
                    width: 100%;
                    height: auto;
                    position: absolute;
                    left: 50%;
                    top: 0;
                    margin-left: -640px;
