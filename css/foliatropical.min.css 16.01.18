        header#topo.index .wrapper p.us { font-size: 30px; }
            header#topo.index .wrapper p strong {
                display: block;
                font-size: 70px;
                line-height: 1em;
                letter-spacing: -5px;
                color: #fffbc3;
            }

            header#topo.index .wrapper .parallax {
                position: absolute;
                left: 0;
                top: 0;
